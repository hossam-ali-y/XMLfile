@charset "utf-8";

body { }

body::before { box-shadow: rgba(0, 0, 0, 0.07) 5px 0px 7px 0px; }

.site-header { padding: 5% 7.6923%; }

.site-info { display: none; }

.main-navigation .menu-item-description { font-family: "Open Sans", sans-serif; }

body, button, input, select, textarea { font-family: "Open Sans", sans-serif; font-size: 1.5rem; line-height: 1.6; }

h1, h2, h3, h4, h5, h6 { font-family: Candara, serif; font-style: italic; font-weight: 400; }

.site-title { font-family: Candara, serif; font-style: italic; }

.site-description { font-family: "Open Sans", sans-serif; margin: 0.25em 0px 0px; }

.entry-title { font-size: 2.375rem; line-height: 1.1; margin-bottom: 2rem; }

code { color: rgb(218, 133, 91); }

pre code { color: rgb(51, 51, 51); }

.page-header { padding: 2% 8%; }

blockquote { font-size: 1.5rem; }

blockquote cite, blockquote small { font-size: 1.25rem; }

.sticky-post { background-color: rgb(0, 0, 0); color: rgb(255, 255, 255); font-weight: 500; letter-spacing: 0.125em; padding: 0.375em 2em 0.313em; }

.entry-content, .entry-summary { padding: 0px 7% 10%; }

.entry-content a, .entry-summary a, .page-content a, .comment-content a, .pingback .comment-body > a { font-weight: 700; color: rgb(0, 0, 0); }

.entry-content .more-link::after, .entry-summary .more-link::after { content: "..."; font-size: initial; line-height: 1; top: 2px; }

.entry-content .more-link { padding: 0px; font-family: Candara; font-size: 1.5rem; font-weight: 400; font-style: italic; text-transform: capitalize; }

.entry-content a.more-link, .entry-summary a.more-link { text-decoration: none; border-bottom: none; }

.entry-content a.more-link:hover, .entry-summary a.more-link:focus { color: rgb(112, 112, 112); }

.social-navigation a::before { background-color: rgb(236, 236, 236); padding: 0.313em; border-radius: 50%; }

.pagination .next::before { right: 0px; }

.pagination .prev, .pagination .next { -webkit-tap-highlight-color: rgba(255, 255, 255, 0.3); background-color: rgb(247, 247, 247); color: rgb(51, 51, 51); }

.pagination .prev:hover, .pagination .prev:focus, .pagination .next:hover, .pagination .next:focus { background-color: rgb(112, 112, 112); color: rgb(255, 255, 255); }

.dropdown-toggle { top: 0px; width: 37px; height: 37px; }

.main-navigation a { padding: 0.5em 0px; }

@media screen and (min-width: 38.75em) {
  .entry-content, .entry-summary { padding: 0px 9% 7%; }
  .entry-footer { padding: 2% 9%; }
  .banner .widget { width: auto; margin: 0px 7.6923% 6%; }
}

@media screen and (min-width: 46.25em) {
  .entry-title, .widecolumn h2 { font-size: 2.75rem; }
