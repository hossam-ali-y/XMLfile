  .site-header { margin: 12% 0px; }
  .site-title { font-size: 2.2rem; }
  .site-info { padding: 2% 10%; }
  .site-header, .widget { padding-left: 10%; padding-right: 10%; }
  .banner .widget { padding-left: 0px; padding-right: 0px; width: auto; margin: 0px 8.3333% 6%; }
  .main-navigation, .social-navigation { margin-left: 10%; margin-right: 10%; }
  .dropdown-toggle { height: 32px; width: 32px; }
  .entry-content, .entry-summary { padding: 0px 10% 7%; }
