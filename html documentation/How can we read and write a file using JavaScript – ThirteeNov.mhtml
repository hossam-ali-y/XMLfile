}

@media screen and (min-width: 68.75em) {
  .site-title { font-size: 2.688rem; }
  .site-description { font-size: 1.25rem; }
  .site-header, .widget { padding-left: 15%; padding-right: 15%; }
  .main-navigation, .social-navigation { margin-left: 15%; margin-right: 15%; }
  .dropdown-toggle { height: 30px; width: 30px; }
  .entry-title, .widecolumn h2 { font-size: 3rem; }
  blockquote { font-size: 1.75rem; }
  blockquote cite, blockquote small { font-size: 1.25rem; }
}

@media screen and (min-width: 77.5em) {
  .site-title { font-size: 3rem; line-height: 1.125; }
  .site-description { font-size: 1.375rem; }
  .entry-title, .widecolumn h2 { font-size: 3.125rem; }
  .widget { font-size: 1.438rem; }
  .dropdown-toggle { top: 1px; }
}
